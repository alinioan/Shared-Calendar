{
  "realm": "calendar-realm",
  "enabled": true,
  "sslRequired": "external",
  "registrationAllowed": true,
  "loginWithEmailAllowed": true,
  "duplicateEmailsAllowed": false,
  "resetPasswordAllowed": true,
  "editUsernameAllowed": false,
  "bruteForceProtected": true,
  
  "roles": {
    "realm": [
      {
        "name": "member",
        "description": "Can access group calendar and set availability"
      },
      {
        "name": "organizer",
        "description": "Can plan events and manage group members"
      },
      {
        "name": "admin",
        "description": "Full administrative access"
      }
    ]
  },
  
  "clients": [
    {
      "clientId": "calendar-client",
      "name": "Calendar Platform Service",
      "description": "Backend service for calendar platform",
      "enabled": true,
      "publicClient": true,
      "protocol": "openid-connect",
      "standardFlowEnabled": true,
      "implicitFlowEnabled": false,
      "directAccessGrantsEnabled": true
    }
  ],
  "users": [
